#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0/node_modules/next/dist/bin/node_modules:/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0/node_modules/next/dist/node_modules:/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0/node_modules/next/node_modules:/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0/node_modules:/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0/node_modules/next/dist/bin/node_modules:/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0/node_modules/next/dist/node_modules:/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0/node_modules/next/node_modules:/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0/node_modules:/Users/darren/Desktop/Projects/aptogotchi/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
